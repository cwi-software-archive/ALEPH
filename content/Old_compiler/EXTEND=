
          IDENT  EXTEND=
          SST
          ENTRY  EXTEND=
 EXTEND=  JP     *+1S17      SEE MACRO 'EXTEND12'
          SA1    X1+>>       SPECIAL PARAMETER FOR 'EXTEND'
          RJ     =XEXTEND
          SA4    =XADDR>>
          SA5    =XNEW>>
          BX6    X5
          SA6    X4
          SX1    A1->>       RESTORE X1
          EQ     EXTEND=
          END
